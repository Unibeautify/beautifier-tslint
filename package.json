{
  "name": "@unibeautify/beautifier-tslint",
  "version": "0.1.0",
  "description": "TSLint beautifier for Unibeautify",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc",
    "jest": "jest",
    "test": "npm run build && npm run lint && npm run jest",
    "lint": "tslint '{src,test}/**/*.ts' -e '**/fixtures/preferBeautifierConfig/**'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/beautifier-tslint.git"
  },
  "keywords": [
    "eslint",
    "unibeautify",
    "beautifier"
  ],
  "author": "Glavin Wiechert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unibeautify/beautifier-tslint/issues"
  },
  "homepage": "https://github.com/Unibeautify/beautifier-tslint#readme",
  "peerDependencies": {
    "unibeautify": ">= 0.12.1",
    "tslint": "^5.11.0"
  },
  "devDependencies": {
    "@types/node": "12.7.2",
    "@types/jest": "24.0.18",
    "@types/tmp": "0.1.0",
    "jest": "24.9.0",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-clean-code": "0.2.9",
    "tslint-microsoft-contrib": "6.2.0",
    "typescript": "3.6.2",
    "unibeautify": "0.17.0"
  },
  "dependencies": {
    "read-pkg-up": "^6.0.0",
    "tmp": "0.1.0"
  }
}
